		"prepare": "husky"
	},
	"devDependencies": {
		"@fontsource/jetbrains-mono": "5.0.19",
		"@fontsource/manrope": "5.0.19",
		"@fontsource/nanum-pen-script": "5.0.11",
		"@fontsource/permanent-marker": "5.0.8",
		"@fortawesome/fontawesome-free": "7.1.0",
		"@sveltejs/adapter-auto": "3.0.0",
		"@sveltejs/adapter-static": "3.0.1",
		"@sveltejs/kit": "2.5.4",
		"@sveltejs/vite-plugin-svelte": "3.0.0",
		"@types/eslint": "8.56.0",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"husky": "9.0.11",
		"install": "0.13.0",
		"mdsvex": "0.11.0",
		"npm": "^10.8.2",
